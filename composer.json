{
    "name": "hocvt/laravel-opcache",
    "description": "This is my package laravel-opcache",
    "keywords": [
        "hocvt",
        "laravel-opcache"
    ],
    "homepage": "https://github.com/hocvt/laravel-opcache",
    "license": "MIT",
    "authors": [
        {
            "name": "Vu Thai Hoc",
            "email": "vuthaihoc.ttud@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.3",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "HocVT\\LaravelOpcache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HocVT\\LaravelOpcache\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HocVT\\LaravelOpcache\\OpcacheServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
